@article{godfrey-smith_2011,
	abstract = {The commentaries by Dennett, Sterelny, and Queller on Darwinian Populations and Natural Selection ({DPNS)} are so constructive that they make it possible to extend and improve the book's framework in several ways. My replies will focus on points of disagreement, and I will pick a small number of themes and develop them in detail. The three replies below are mostly self-contained, except that all my comments about genes, discussed by all three critics, are in the reply to Queller. Agential views of evolution, discussed by Queller and Dennett, are addressed in my reply to Dennett.},

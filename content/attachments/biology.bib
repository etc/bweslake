@article{mills_1979,
	abstract = {The concept of "fitness" is a notion of central importance to evolutionary theory. Yet the interpretation of this concept and its role in explanations of evolutionary phenomena have remained obscure. We provide a propensity interpretation of fitness, which we argue captures the intended reference of this term as it is used by evolutionary theorists. Using the propensity interpretation of fitness, we provide a Hempelian reconstruction of explanations of evolutionary phenomena, and we show why charges of circularity which have been levelled against explanations in evolutionary theory are mistaken. Finally, we provide a definition of natural selection which follows from the propensity interpretation of fitness, and which handles all the types of selection discussed by biologists, thus improving on extant definitions.},
	author = {Susan K. Mills and John H. Beatty},
	date-added = {2014-01-15 16:41:00 +0000},
	date-modified = {2022-02-02 13:08:16 +0800},

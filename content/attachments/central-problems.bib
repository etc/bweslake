@book{wilkes_1993,
	abstract = {This book explores the scope and limits of the concept of a person --- a vexed question in contemporary philosophy. The author begins by questioning the methodology of thought-experimentation, arguing that it engenders inconclusive and unconvincing results, and that truth is stranger than fiction. She then examines an assortment of real-life conditions, including infancy, insanity and dementia, dissociated states, and split brains. The popular faith in continuity of consciousness and the unity of the person is subjected to sustained criticism. The author concludes with a look at different views of the person found in Homer, Aristotle, the post-Cartesians, and contemporary cognitive science.},
	address = {Oxford},

@article{danks_2013,
	abstract = {Our concept of actual causation plays a deep, ever-present role in our experiences. I first argue that traditional philosophical methods for understanding this concept are unlikely to be successful. I contend that we should instead use functional analyses and an understanding of the cognitive bases of causal cognition to gain insight into the concept of actual causation. I additionally provide initial, programmatic steps towards carrying out such analyses. The characterization of the concept of actual causation that results is quite different from many standard views: it is graded, context-sensitive, and extrinsic.},
	author = {Danks, David},
	date-added = {2020-08-28 20:51:53 -0400},
